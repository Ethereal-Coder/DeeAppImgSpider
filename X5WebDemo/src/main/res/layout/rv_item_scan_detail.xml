<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="3dp">
	<ImageView
			android:id="@+id/iv_Rv_item_scan_detail"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
	</ImageView>

	<TextView
			android:id="@+id/tv_rv_item_img_scan_delete"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:layout_alignParentRight="true"
			android:layout_alignParentTop="true"
			android:background="@mipmap/delete_img"/>
	<LinearLayout
			android:layout_width="50dp"
			android:layout_height="100dp"
			android:layout_alignParentLeft="true"
			android:orientation="vertical"
			android:weightSum="2">
		<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1">
			<TextView
					android:id="@+id/tv_rv_item_move_up"
					android:layout_width="40dp"
					android:layout_height="40dp"
					android:layout_centerInParent="true"
					android:background="@mipmap/arow_up"/>
		</RelativeLayout>
		<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1">
			<TextView
					android:id="@+id/tv_rv_item_move_down"
					android:layout_width="40dp"
					android:layout_height="40dp"
					android:layout_centerInParent="true"
					android:background="@mipmap/arow_down"/>
		</RelativeLayout>

	</LinearLayout>
</RelativeLayout>
